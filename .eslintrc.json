{
  "parser": "babel-eslint",
  "env": {
    "mocha": true,
    "node": true
  },
  "globals": {},
  "rules": {
    "no-bitwise": 0,
    "curly": 0,
    "eqeqeq": 2,
    "no-eq-null": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "no-use-before-define": 2,
    "new-cap": 2,
    "no-caller": 2,
    "no-undef": 2,
    "strict": 0
  }
}
